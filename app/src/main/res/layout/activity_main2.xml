<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFEFE"
    tools:context=".MainActivity2">

    <Button
        android:id="@+id/ypkal"
        android:layout_width="342dp"
        android:layout_height="55dp"
        android:layout_marginBottom="405dp"
        android:background="@drawable/rounded"
        android:text="ΥΠΗΡΕΣΙΕΣ ΚΑΛΑΜΑΤΑΣ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ypmeg" />

    <Button
        android:id="@+id/ypmeg"
        android:layout_width="342dp"
        android:layout_height="55dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="317dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/rounded"
        android:text="ΥΠΗΡΕΣΙΕΣ ΜΕΓΑΛΟΠΟΛΗΣ"
        app:layout_constraintBottom_toTopOf="@+id/ypkal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="402dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="612dp"
        android:fontFamily="sans-serif-smallcaps"
        android:text="ΚΛΗΣΕΙΣ ΕΚΤΑΚΤΗΣ ΑΝΑΓΚΗΣ"
        android:textAlignment="center"
        android:textSize="29sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="35dp"
        app:layout_constraintBottom_toTopOf="@+id/ypmeg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/facebook_cover___1" />


</androidx.constraintlayout.widget.ConstraintLayout>